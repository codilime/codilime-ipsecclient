{{range .}}
{{.ID}}-{{.ClientName}}-{{.TunnelNumber}} {
	remote_addrs = {{.RemoteIPSec}}

	local {
		
	}
	remote {
		auth = psk
	}
	children {
		site-cisco {
			remote_ts = 0.0.0.0/0
			local_ts = 0.0.0.0/0
			if_id_in = {{.ID}}
			if_id_out = {{.ID}}
			esp_proposals = {{.CryptoPH1}}
			start_action=start
		}
	}
	version = 2
	proposals = {{.CryptoPH1}}
	}
}

secrets {
	ike-cisco {
		secret = {{.PSK}}
	}
}
{{end}}
